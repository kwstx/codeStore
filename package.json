{
  "name": "engram",
  "displayName": "Engram",
  "description": "The gateway to code memory. Local-first semantic index and context retrieval.",
  "readme": "README.md",
  "version": "0.0.6",
  "publisher": "use-engram",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.96.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kwstx/Engram"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "engram.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "engram.excludeFile",
        "title": "Engram: Exclude this File"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "engram-sidebar",
          "title": "Engram",
          "icon": "images/icon.png"
        }
      ]
    },
    "views": {
      "engram-sidebar": [
        {
          "type": "webview",
          "id": "engram.promptTimeline",
          "name": "Prompt Timeline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.17.0",
    "glob": "^8.1.0",
    "mocha": "^11.0.1",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@types/diff": "^7.0.2",
    "@xenova/transformers": "^2.14.0",
    "diff": "^8.0.2",
    "uuid": "^11.0.4",
    "vectordb": "^0.4.5"
  }
}